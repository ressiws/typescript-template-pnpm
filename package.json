{
	"name": "typescript-project-template-pnpm",
	"version": "1.0.0",
	"description": "A simple typescript template",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"start": "node .",
		"dev": "tsx watch src/index.ts",
		"lint": "npx eslint .",
		"backup": "pnpm scripts ./scripts/backup.mts",
		"build": "pnpm run clean && pnpm exec tsc && pnpm run dotbuild .build/postprocess.build.mts",
		"clean": "pnpm run dotbuild .build/clean.build.mts",
		"dotbuild": "pnpm exec tsx --tsconfig=.build/tsconfig.json",
		"scripts": "pnpm exec tsx --tsconfig=scripts/tsconfig.json"
	},
	"keywords": [],
	"authors": [
		"DarkenLM",
		"ressiws"
	],
	"license": "MIT",
	"dependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/migrate-config": "^1.3.1",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"archiver": "^7.0.1",
		"cac": "^6.7.14",
		"chalk": "^5.3.0",
		"dotenv": "^16.4.5",
		"tsx": "^4.19.1",
		"winston": "^3.15.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@types/archiver": "^6.0.3",
		"@types/node": "^22.7.5",
		"eslint": "^9.12.0",
		"globals": "^15.11.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.9.0"
	}
}